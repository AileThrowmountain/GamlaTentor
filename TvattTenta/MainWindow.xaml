<Window x:Class="TvattTenta.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TvattTenta"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="17*"/>
            <ColumnDefinition Width="55*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="137,108,0,0" VerticalAlignment="Top" Width="75" Click="button_Click" Grid.Column="1"/>
        <Button x:Name="button1" Content="Fredag" HorizontalAlignment="Left" Margin="144,239,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click" Grid.ColumnSpan="2"/>
        <Button x:Name="button3" Content="Lördag" HorizontalAlignment="Left" Margin="144,264,0,0" VerticalAlignment="Top" Width="75" Grid.ColumnSpan="2"/>
        <ListBox x:Name="listBoxKläder" HorizontalAlignment="Left" Height="144" Margin="23,140,0,0" VerticalAlignment="Top" Width="116"/>

    </Grid>
</Window>
