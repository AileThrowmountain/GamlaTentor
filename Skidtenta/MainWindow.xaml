<Window x:Class="Skidtenta.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Skidtenta"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="label1" Content="Längd" HorizontalAlignment="Left" Margin="116,91,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxLängd" HorizontalAlignment="Left" Height="23" Margin="116,122,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="buttonStav" Content="Stavlängd" HorizontalAlignment="Left" Margin="116,158,0,0" VerticalAlignment="Top" Width="75" Click="buttonStav_Click"/>
        <Button x:Name="buttonVallaburk" Content="Valla" HorizontalAlignment="Left" Margin="116,313,0,0" VerticalAlignment="Top" Width="75" Click="buttonVallaburk_Click"/>
        <TextBox x:Name="textBoxValla" HorizontalAlignment="Left" Height="23" Margin="116,285,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label2" Content="Temperatur" HorizontalAlignment="Left" Margin="116,259,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
